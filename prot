/dev/input/event3 -> KeyboardPath

A -> B

ctrl+a -> (blockKeys: True) remap
ctrl+b => (blockKeys: True) script

lshift+rshift -> CapsLock

SUPER <-> swap

CapsLock -> NewLayer(onTap -> CapsLock)
  SPACE -> TAB
    H -> LEFT
    J -> DOWN
    K -> UP
    L -> RIGHT

CapsLock -> NewLayer(onTap => echo ",")
  SPACE -> TAB
    H -> LEFT
    J -> DOWN
    K -> UP
    L -> RIGHT

